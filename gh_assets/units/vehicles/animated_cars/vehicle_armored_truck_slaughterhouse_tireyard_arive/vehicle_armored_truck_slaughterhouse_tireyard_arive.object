<?xml version="1.0"?>
<dynamic_object>
  <diesel materials="units/vehicles/animated_cars/vehicle_armored_truck_slaughterhouse_tireyard_arive/vehicle_armored_truck_slaughterhouse_tireyard_arive" orientation_object="rp_police_truck" />
  <sequence_manager file="units/vehicles/animated_cars/vehicle_armored_truck_slaughterhouse_tireyard_arive/vehicle_armored_truck_slaughterhouse_tireyard_arive" />
	<animations>
	
		<!-- Slaughterhouyse Ambush Event Animations -->
		<animation_group name="main_animation">
			<object name="a_main_animation" />
			<object name="a_body" />
			<object name="a_front_right_wheel" />
			<object name="a_front_left_wheel" />
			<object name="a_rear_wheels" />
		</animation_group>
		<animation_group name="crash1">
			<object name="a_crash1" />
			<object name="a_body_crash1" />
			<object name="a_rear_wheels_crash1" />
			<object name="a_front_left_wheel_crash1" />
			<object name="a_front_right_wheel_crash1" />
		</animation_group>
		
		<animation_group name="crash2">
			<object name="a_crash2" />
			<object name="a_body_crash2" />
			<object name="a_rear_wheels_crash2" />
			<object name="a_front_left_wheel_crash2" />
			<object name="a_front_right_wheel_crash2" />
		</animation_group>
		<!-- //Slaughterhouyse Ambush Event Animations END -->
		
		<animation_group name="ag_right_door">
			<object name="a_right_door" />
		</animation_group>

		<animation_group name="ag_left_door">
			<object name="a_left_door" />
		</animation_group>
		
		<animation_group name="ag_tire_front_right">
		  <object name="a_tire_front_right" />
		</animation_group>
		<animation_group name="ag_tire_front_left">
		  <object name="a_tire_front_left" />
		</animation_group>
		<animation_group name="ag_tire_rear_right">
		  <object name="a_tire_rear_right" />
		</animation_group>
		<animation_group name="ag_tire_rear_left">
		  <object name="a_tire_rear_left" />
		</animation_group>
	</animations>
	
	<bodies>
		<body name="animated_body" enabled="true" template="animated">
		  <object name="a_body_crash2"/>
		  <object name="c_convex_police_truck_1" collision_type="convex" padding="-2"/>
		  <object name="c_convex_police_truck_2" collision_type="convex" padding="-2"/>
		  <object name="c_convex_police_truck_3" collision_type="convex" padding="-2"/>
		</body>
		<body remove_ray_type="body" name="player_nono" enabled="true" template="animated">
		  <object name="c_player_nono" collision_type="box" />
		</body>
		
		<body remove_ray_type="ai_vision" name="window_front_left" enabled="true" template="animated">
		  <object name="c_window_front_left" collision_type="box" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_front_right" enabled="true" template="animated">
		  <object name="c_window_front_right" collision_type="box" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_right" enabled="true" template="animated">
		  <object name="c_window_right" collision_type="convex" padding="-2"/>
		</body>
		<body remove_ray_type="ai_vision" name="window_left" enabled="true" template="animated">
		  <object name="c_window_left" collision_type="convex" padding="-2"/>
		</body>
		 <body remove_ray_type="ai_vision" name="window_right_door" enabled="true" template="animated">
		  <object name="c_window_right_door" collision_type="box" padding="-2"/>
		</body>
		 <body remove_ray_type="ai_vision" name="window_left_door" enabled="true" template="animated">
		  <object name="c_window_left_door" collision_type="box" padding="-2"/>
		</body>
		
		<body name="tire_front_left" enabled="true" template="animated">
		  <object name="c_tire_front_left" collision_type="convex" padding="-2"/>
		</body>
		<body name="tire_rear_left" enabled="true" template="animated">
		  <object name="c_tire_rear_left" collision_type="convex" padding="-2"/>
		</body>
		<body name="tire_front_right" enabled="true" template="animated">
		  <object name="c_tire_front_right" collision_type="convex" padding="-2"/>
		</body>
		<body name="tire_rear_right" enabled="true" template="animated">
		  <object name="c_tire_rear_right" collision_type="convex" padding="-2"/>
		</body>
		
		<body name="static_body2" enabled="true" template="animated">
		  <object name="a_left_door" />
		  <object name="c_left_door1" collision_type="box" padding="-2"/>
		  <object name="c_left_door2" collision_type="box" padding="-2"/>
		  <object name="c_left_door3" collision_type="box" padding="-2"/>
		  <object name="c_left_door4" collision_type="box" padding="-2"/>
		</body>
		
		<body name="static_body3" enabled="true" template="animated">
		  <object name="a_right_door" />
		  <object name="c_right_door1" collision_type="box" padding="-2"/>
		  <object name="c_right_door2" collision_type="box" padding="-2"/>
		  <object name="c_right_door3" collision_type="box" padding="-2"/>
		  <object name="c_right_door3" collision_type="box" padding="-2"/>
		</body>

	</bodies>
  
	<decal_surfaces default_material="metal">
    <decal_mesh name="dm_police_truck" 			enabled="true" material="metal_chassis"/>
    <decal_mesh name="dm_left_door" 			enabled="true" material="metal_chassis" />
    <decal_mesh name="dm_right_door" 			enabled="true" material="metal_chassis" />
	<decal_mesh name="dm_tier" 					enabled="true" material="rubber" />
	<decal_mesh name="dm_window_right_door" 	enabled="true" material="glass_breakable" />
	<decal_mesh name="dm_window_left_door" 		enabled="true" material="glass_breakable" />
	<decal_mesh name="dm_window_right" 			enabled="true" material="glass_breakable" />
	<decal_mesh name="dm_window_left" 			enabled="true" material="glass_breakable" />
	<decal_mesh name="dm_window_front_left" 	enabled="true" material="glass_breakable" />
	<decal_mesh name="dm_window_front_right" 	enabled="true" material="glass_breakable" />
   
  </decal_surfaces>
  
  <graphics>
    <object name="g_police_truck" 				enabled="true" />
    <object name="g_rear_wheels" 				enabled="true" />
	<object name="g_front_left_wheel" 			enabled="true" />
	<object name="g_front_right_wheel" 			enabled="true" />
	<object name="g_window_right" 				enabled="true" />
	<object name="g_window_left" 				enabled="true" />
	<object name="g_window_front_left" 			enabled="true" />
	<object name="g_window_front_right" 		enabled="true" />
	<object name="g_window_right_dmg" 			enabled="false" />
	<object name="g_window_left_dmg" 			enabled="false" />
	<object name="g_window_front_left_dmg" 		enabled="false" />
	<object name="g_window_front_right_dmg" 	enabled="false" />
	<object name="g_fence" 						enabled="true" />
	<object name="g_left_door" 					enabled="true" />
	<object name="g_window_left_door" 			enabled="true" />
	<object name="g_window_left_door_dmg" 		enabled="false" />
	<object name="g_right_door" 				enabled="true" />
	<object name="g_window_right_door" 			enabled="true" />
	<object name="g_window_right_door_dmg" 		enabled="false" />
	<object name="g_under_plate" 				enabled="true" />
	<object name="g_cone_light" 				enabled="false" instance_struct="world_tm" />
	<object name="g_illum_off" 					enabled="true" />
	<object name="g_illum_on" 					enabled="false" />
	<object name="g_floater" 					enabled="true" />
	<object name="g_door_floater_r" 			enabled="true" />
	<object name="g_door_floater_l" 			enabled="true" />
	<object name="s_police_truck" 				enabled="true" shadow_caster="true" />
	<object name="s_left_door" 					enabled="true" shadow_caster="true" />
	<object name="s_right_door" 				enabled="true" shadow_caster="true" />
	
  </graphics>
  
   <lights>
	   <light name="ls_left" far_range="2000" type="spot|specular"   end_angle="53" multiplier="identity" editable="true" />
	   <light name="ls_right" far_range="2000" type="spot|specular"   end_angle="39" multiplier="identity" editable="true" />
	   <light name="ls_t_left" far_range="1000" type="spot|specular"  end_angle="53" multiplier="identity" editable="true" />
	   <light name="ls_t_right" far_range="1000" type="spot|specular" end_angle="39" multiplier="identity" editable="true" />
  </lights>
</dynamic_object>